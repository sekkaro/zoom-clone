{
  "name": "zoom-clone",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "copyfiles -u 1 -e src/**/*.ts src/**/*.* dist/ && browserify -e dist/public/script.js -o dist/public/bundle.js && node dist/index.js",
    "dev": "tsc-watch --onSuccess \"yarn build\"",
    "dev3": "nodemon dist/index.js",
    "start": "node dist/index.js",
    "start2": "ts-node src/index.ts",
    "dev2": "nodemon --exec ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.0",
    "@types/express": "^4.17.13",
    "@types/jquery": "^3.5.6",
    "@types/node": "^16.6.2",
    "@types/uuid": "^8.3.1",
    "browserify": "^17.0.0",
    "copyfiles": "^2.4.1",
    "ts-node": "^10.2.1",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "peer": "^0.6.1",
    "peerjs": "^1.3.2",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "uuid": "^8.3.2"
  }
}
